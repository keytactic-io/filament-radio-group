{
    "name": "keytactic-io/filament-radio-group",
    "description": "A radio group field for Filament with support for icons and custom colors.",
    "keywords": [
        "filament",
        "laravel",
        "filament-radio-group"
    ],
    "homepage": "https://github.com/keytactic-io/filament-radio-group",
    "license": "MIT",
    "authors": [
        {
            "name": "astersnake",
            "email": "astersnake@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Mo Hassan",
            "email": "mo@keytactic.net",
            "role": "Maintainer"
        }        
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^3.0",
        "spatie/laravel-package-tools": "^1.13.5",
        "illuminate/contracts": "^10.0|^11.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^7.0",
        "nunomaduro/larastan": "^2.0",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "pestphp/pest-plugin-livewire": "^2.0",
        "phpstan/extension-installer": "^1.2",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.2",
        "phpunit/phpunit": "^10.5",
        "spatie/laravel-ray": "^1.36"
    },
    "autoload": {
        "psr-4": {
            "Keytactic\\RadioGroup\\": "src",
            "Keytactic\\RadioGroup\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Keytactic\\RadioGroup\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test:pest": "vendor/bin/pest",
        "test:phpstan": "vendor/bin/phpstan analyse",
        "test": [
            "@test:pest",
            "@test:phpstan"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ketyactic\\RadioGroup\\RadioGroupServiceProvider"
            ],
            "aliases": {
                "RadioGroup": "Keytactic\\RadioGroup\\Facades\\RadioGroup"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}